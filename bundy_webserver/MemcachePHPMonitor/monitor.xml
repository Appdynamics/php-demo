<monitor>
    <name>MemcacheMonitor</name>
    <type>managed</type>
    <description>Monitors Memcache via PHP</description>
    <monitor-configuration></monitor-configuration>
    <monitor-run-task>
        <execution-style>periodic</execution-style>
        <execution-frequency-in-seconds>60</execution-frequency-in-seconds>
        <name>Memcache Monitor Run Task</name>
        <display-name>Memcache Status Monitor Task</display-name>
        <description>Memcache Status Monitor Task</description>
        <type>executable</type>
        <task-arguments></task-arguments>
        <executable-task>
            <type>file</type>
            <file os-type="linux">memcacheMonitor.sh</file>
        </executable-task>
     </monitor-run-task>
</monitor>
